	author = {Stodden, Victoria and McNutt, Marcia and Bailey, David H. and Deelman, Ewa and Gil, Yolanda and Hanson, Brooks and Heroux, Michael A. and Ioannidis, John P. A. and Taufer, Michela},
	month = dec,
	year = {2016},
	pmid = {27940837},
	pages = {1240--1241},
}

