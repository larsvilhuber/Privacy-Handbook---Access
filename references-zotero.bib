	url = {https://journalprivacyconfidentiality.org/index.php/jpc/article/view/755},
	doi = {10.29012/jpc.755},
	abstract = {Differential privacy provides a rigorous framework for privacy-preserving data analysis. This paper proposes the first differentially private procedure for controlling the false discovery rate (FDR) in multiple hypothesis testing. Inspired by the Benjamini-Hochberg procedure (BHq), our approach is to first repeatedly add noise to the logarithms of the p-values to ensure differential privacy and to select an approximately smallest p-value serving as a promising candidate at each iteration; the selected p-values are further supplied to the BHq and our private procedure releases only the rejected ones. Moreover, we develop a new technique that is based on a backward submartingale for proving FDR control of a broad class of multiple testing procedures, including our private procedure, and both the BHq step- up and step-down procedures. As a novel aspect, the proof works for arbitrary dependence between the true null and false null test statistics, while FDR control is maintained up to a small multiplicative factor.},
	language = {en},
	number = {2},
	urldate = {2022-01-09},
	journal = {Journal of Privacy and Confidentiality},
	author = {Dwork, Cynthia and Su, Weijie and Zhang, Li},
	month = sep,
	year = {2021},
	keywords = {Benjamini– Hochberg procedure, Differential privacy, Report Noisy Max, false discovery rate, positive regression dependence on subset, submartingale},
}

@inproceedings{narayanan_robust_2008,
	title = {Robust {De}-anonymization of {Large} {Sparse} {Datasets}},
	doi = {10.1109/SP.2008.33},
	abstract = {We present a new class of statistical de- anonymization attacks against high-dimensional micro-data, such as individual preferences, recommendations, transaction records and so on. Our techniques are robust to perturbation in the data and tolerate some mistakes in the adversary's background knowledge. We apply our de-anonymization methodology to the Netflix Prize dataset, which contains anonymous movie ratings of 500,000 subscribers of Netflix, the world's largest online movie rental service. We demonstrate that an adversary who knows only a little bit about an individual subscriber can easily identify this subscriber's record in the dataset. Using the Internet Movie Database as the source of background knowledge, we successfully identified the Netflix records of known users, uncovering their apparent political preferences and other potentially sensitive information.},
	booktitle = {2008 {IEEE} {Symposium} on {Security} and {Privacy} (sp 2008)},
	author = {Narayanan, Arvind and Shmatikov, Vitaly},
	month = may,
