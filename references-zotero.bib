	author = {Goroff, Daniel and Polonetsky, Jules and Tene, Omer},
	month = jan,
	year = {2018},
	note = {Publisher: SAGE Publications Inc},
	keywords = {administrative data, big data, data protection, data research, ethics, privacy},
	pages = {46--66},
}

